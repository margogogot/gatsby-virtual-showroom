{
  "name": "startup",
  "private": false,
  "description": "A startup template perfect for brochure sites and small businesses",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "prebuild": "npm run clean && npm run zip",
    "build": "gatsby build",
    "build-dev": "gatsby build",
    "clean": "rm -rf functions-build && mkdir functions-build",
    "zip": "cd functions/submission-created && yarn install && zip -r submission-created.zip *",
    "postzip": "mv functions/submission-created/submission-created.zip functions-build",
    "copy-showroom": "aws s3 cp s3://fs-virtual-showroom/app1k8/ ./static/showroom/ --exclude 'AR/*' --recursive",
    "develop": "gatsby develop -H 0.0.0.0",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve -H 0.0.0.0 -p 8000",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1"
  },
  "dependencies": {
    "babel-plugin-styled-components": "^1.11.1",
    "framer-motion": "^1.11.1",
    "gatsby": "^2.24.47",
    "gatsby-background-image": "^0.8.19",
    "gatsby-image": "^2.4.16",
    "gatsby-plugin-favicon": "^3.1.6",
    "gatsby-plugin-react-helmet": "^3.3.10",
    "gatsby-plugin-sass": "^2.3.18",
    "gatsby-plugin-sharp": "^2.6.27",
    "gatsby-plugin-sitemap": "^2.4.11",
    "gatsby-plugin-styled-components": "^3.3.10",
    "gatsby-source-filesystem": "^2.3.24",
    "gatsby-transformer-sharp": "^2.5.13",
    "modernizr": "^3.11.3",
    "node-sass": "^4.14.1",
    "normalize.css": "^8.0.1",
    "react": "^16.13.1",
    "react-cookie": "^4.0.3",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.1.0",
    "react-icons": "^3.10.0",
    "react-intersection-observer": "^8.26.2",
    "react-responsive-carousel": "^3.2.10",
    "react-scroll": "^1.8.1",
    "react-share": "^4.3.1",
    "react-sticky": "^6.0.3",
    "react-useragent": "^1.1.2",
    "react-visibility-sensor": "^5.1.1",
    "styled-components": "^4.4.1",
    "typeface-gothic-a1": "0.0.61"
  },
  "devDependencies": {
    "prettier": "^1.19.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bagseye/startup"
  },
  "bugs": {
    "url": "https://github.com/bagseye/startup/issues"
  }
}
